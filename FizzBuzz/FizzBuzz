





public static string FizzBuzz(int number)
{
   Console.WriteLine("Please select a number.");
   number = int.Parse(Console.ReadLine());
   Console.WriteLine(FizzBuzz(userInput));
    
    var answer;

    if(number % 3 == 0)
    {
        answer += "fizz";
    }
    if(number % 5 == 0)
    {
        answer += "buzz";
    }
    else
    {
        answer = "no fizzbuzz for you!";
    }

    return answer;

}